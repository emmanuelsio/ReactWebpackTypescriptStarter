{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "es5",
        "removeComments": true,
        "alwaysStrict": true,
        "allowUnreachableCode": false,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true
    }
}